{
  "name": "@funq/mock-server",
  "version": "0.0.3",
  "description": "RESTful Mock server with file upload and download support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pure-Function/mock-server.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mock-server",
    "json",
    "file-upload",
    "file-download",
    "extendable"
  ],
  "author": "Pure Function",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pure-Function/mock-server/issues"
  },
  "homepage": "https://github.com/Pure-Function/mock-server#readme",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^24.9.1",
    "@types/moxios": "^0.4.9",
    "@types/node": "^12.12.68",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.1.0",
    "jest": "^26.5.3",
    "lint-staged": "^9.5.0",
    "moxios": "^0.4.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "obsolete:prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint '{src,__test__}/**/*.{js,ts,tsx}' --quiet --fix",
    "prestart": "npm run build",
    "start": "node .",
    "dev:server": "nodemon",
    "dev:server-debug": "node -r ts-node/register --inspect=5858 src/index.ts",
    "test": "jest",
    "dev:tdd": "node --inspect=5859 node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "@types/express-fileupload": "^1.1.5",
    "@types/lodash": "^4.14.162",
    "@types/mime-types": "^2.1.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "nodemon": "^2.0.5"
  }
}
